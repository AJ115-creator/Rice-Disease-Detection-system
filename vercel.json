{
  "version": 2,
  "builds": [
    {
      "src": "Frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "Frontend/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/Frontend/$1"
    }
  ],
  "buildCommand": "cd Frontend && npm install && npm run build",
  "outputDirectory": "Frontend/dist"
}

